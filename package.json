{
  "name": "eav-reporter",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "repository": "git@github.com:Yan-Boogie/EAV-Reporter.git",
  "author": "Ian Lai <ian24257587@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:rollup": "rollup --config ./rollup.config.js",
    "build:rollup:watch": "yarn build:rollup --watch",
    "client:start": "yarn workspace client start",
    "client:build": "yarn workspace client build",
    "client:analyze-dev": "yarn workspace client analyze-dev",
    "client:analyze": "yarn workspace client analyze",
    "commit": "git-cz",
    "lint:ts": "eslint . --config .eslintrc --ext .js,.ts",
    "server:start": "yarn workspace server start"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.4",
    "prettier": "^2.2.1",
    "rollup": "^2.26.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "^4.2.3"
  }
}
